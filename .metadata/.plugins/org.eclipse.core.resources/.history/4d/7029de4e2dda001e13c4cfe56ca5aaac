package persistence.dto;

import java.time.LocalDate;
import java.time.LocalDateTime;

public class ItemByDateDto {
	public static String PROP_IG_ID = "id";
	public static String PROP_IG_NAME = "name";
	public static String PROP_CREATED_AT = "created_time";
	
	private Integer id;
	private String name;
	private LocalDate created_time;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public LocalDate getCreated_at() {
		return created_time;
	}

	public void setCreated_at(LocalDate created_at) {
		this.created_time = created_at;
	}

	public ItemByDateDto(Integer id, String name, LocalDate created_time) {

		this.id = id;
		this.name = name;
		this.created_time = created_time;
	}
	
	

	public ItemByDateDto() {
	}

	@Override
	public String toString() {
		return "ItemByDateDto [id=" + id + ", name=" + name + ", created_time=" + created_time + "]";
	}
	
	
}
