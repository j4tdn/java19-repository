		// 1st level cache
		// configure : có sẵn của hibernate
		// disable : không được
		// scope : single transaction - single session
		
		Session s1 = openSession();
		Session s2 = openSession();
	}

	@Override
	public void secondLevelCache() {
		
	}
	
}
