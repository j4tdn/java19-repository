package persistence.entities;

import javax.persistence.Column;
import javax.persistence.Id;

public class Size {
	@Id
	@Column(name = "ID")
	private Integer id;
	
	@Column(name = "SIZE")
	private String size;
	@Column(name = "GENDER")
	private Boolean gender;
	
	@Column(name = "DESC")
	private String desciption;
	
	public Size() {
	}
}
